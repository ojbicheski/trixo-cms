<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
	<application>
		<locale-config>
			<default-locale>pt-BR</default-locale>
		</locale-config>
		<resource-bundle>
			<base-name>br.com.trixo.screen.Screens</base-name>
			<var>screen</var>
		</resource-bundle>
	</application>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>painelUsuario</from-outcome>
			<to-view-id>/painelUsuario.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>painelSite</from-outcome>
			<to-view-id>/painelSite.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/painelUsuario.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{siteEntity.newEntity}</from-action>
			<from-outcome>new</from-outcome>
			<to-view-id>/formularioSite.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/painelSite.xhtml</from-view-id>
		<!-- Site -->
		<navigation-case>
			<from-action>#{siteEntity.view}</from-action>
			<from-outcome>view</from-outcome>
			<to-view-id>/formularioSite.xhtml</to-view-id>
		</navigation-case>
		<!-- Categoria -->
		<navigation-case>
			<from-action>#{categoriaEntity.newEntity}</from-action>
			<from-outcome>new</from-outcome>
			<to-view-id>/formularioCategoria.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{categoriaEntity.view}</from-action>
			<from-outcome>view</from-outcome>
			<to-view-id>/formularioCategoria.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{categoriaEntity.remove}</from-action>
			<from-outcome>removed</from-outcome>
			<to-view-id>/painelSite.xhtml</to-view-id>
		</navigation-case>
		<!-- Template -->
		<navigation-case>
			<from-action>#{templateEntity.newEntity}</from-action>
			<from-outcome>new</from-outcome>
			<to-view-id>/formularioTemplate.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{templateEntity.view}</from-action>
			<from-outcome>view</from-outcome>
			<to-view-id>/formularioTemplate.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{templateEntity.remove}</from-action>
			<from-outcome>removed</from-outcome>
			<to-view-id>/painelSite.xhtml</to-view-id>
		</navigation-case>
		<!-- Pagina -->
		<navigation-case>
			<from-action>#{paginaEntity.newEntity}</from-action>
			<from-outcome>new</from-outcome>
			<to-view-id>/formularioPagina.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{paginaEntity.view}</from-action>
			<from-outcome>view</from-outcome>
			<to-view-id>/formularioPagina.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{paginaEntity.remove}</from-action>
			<from-outcome>removed</from-outcome>
			<to-view-id>/painelSite.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{selecionarPagina.selecionar(paginaEntity.id)}</from-action>
			<to-view-id>/painelPagina.xhtml</to-view-id>
		</navigation-case>
		<!-- Conteudo -->
		<navigation-case>
			<from-action>#{paginaEntity.viewConteudo}</from-action>
			<from-outcome>painel</from-outcome>
			<to-view-id>/painelPagina.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>atualizacoesSite</from-outcome>
			<to-view-id>/painelPublicacao.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/formularioSite.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{siteEntity.persist}</from-action>
			<from-outcome>persisted</from-outcome>
			<to-view-id>/painelUsuario.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{siteEntity.update}</from-action>
			<from-outcome>updated</from-outcome>
			<to-view-id>/painelSite.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/formularioCategoria.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{categoriaEntity.persist}</from-action>
			<from-outcome>persisted</from-outcome>
			<to-view-id>/painelSite.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{categoriaEntity.update}</from-action>
			<from-outcome>updated</from-outcome>
			<to-view-id>/painelSite.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/formularioTemplate.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{templateEntity.persist}</from-action>
			<from-outcome>persisted</from-outcome>
			<to-view-id>/painelSite.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{templateEntity.update}</from-action>
			<from-outcome>updated</from-outcome>
			<to-view-id>/painelSite.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/formularioPagina.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{paginaEntity.persist}</from-action>
			<from-outcome>persisted</from-outcome>
			<to-view-id>/painelSite.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{paginaEntity.update}</from-action>
			<from-outcome>updated</from-outcome>
			<to-view-id>/painelSite.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/painelPagina.xhtml</from-view-id>
		<!-- Conteudo: Texto Curto -->
		<navigation-case>
			<from-outcome>newTextoCurto</from-outcome>
			<to-view-id>/formularioTextoCurto.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>viewTextoCurto</from-outcome>
			<to-view-id>/formularioTextoCurto.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>removedTextoCurto</from-outcome>
			<to-view-id>/painelPagina.xhtml</to-view-id>
		</navigation-case>
		<!-- Conteudo: Texto Longo -->
		<navigation-case>
			<from-outcome>newTextoLongo</from-outcome>
			<to-view-id>/formularioTextoLongo.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>viewTextoLongo</from-outcome>
			<to-view-id>/formularioTextoLongo.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>removedTextoLongo</from-outcome>
			<to-view-id>/painelPagina.xhtml</to-view-id>
		</navigation-case>
		<!-- Conteudo: Link Interno -->
		<navigation-case>
			<from-outcome>newLinkInterno</from-outcome>
			<to-view-id>/formularioLinkInterno.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>viewInterno</from-outcome>
			<to-view-id>/formularioInterno.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>removedInterno</from-outcome>
			<to-view-id>/painelPagina.xhtml</to-view-id>
		</navigation-case>
		<!-- Conteudo: Link Externo -->
		<navigation-case>
			<from-outcome>newLinkExterno</from-outcome>
			<to-view-id>/formularioLinkExterno.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>viewLinkExterno</from-outcome>
			<to-view-id>/formularioLinkExterno.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>removedLinkExterno</from-outcome>
			<to-view-id>/painelPagina.xhtml</to-view-id>
		</navigation-case>
		<!-- Conteudo: Download -->
		<navigation-case>
			<from-outcome>newDownload</from-outcome>
			<to-view-id>/formularioDownload.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>viewDownload</from-outcome>
			<to-view-id>/formularioDownload.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>removedDownload</from-outcome>
			<to-view-id>/painelPagina.xhtml</to-view-id>
		</navigation-case>
		<!-- Conteudo: Imagem -->
		<navigation-case>
			<from-outcome>newImagem</from-outcome>
			<to-view-id>/formularioImagem.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>viewImagem</from-outcome>
			<to-view-id>/formularioImagem.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>removedImagem</from-outcome>
			<to-view-id>/painelPagina.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/formularioTextoCurto.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{textoCurtoEntity.persist}</from-action>
			<from-outcome>persisted</from-outcome>
			<to-view-id>/painelPagina.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{textoCurtoEntity.update}</from-action>
			<from-outcome>updated</from-outcome>
			<to-view-id>/painelPagina.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/formularioTextoLongo.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{textoLongoEntity.persist}</from-action>
			<from-outcome>persisted</from-outcome>
			<to-view-id>/painelPagina.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{textoLongoEntity.update}</from-action>
			<from-outcome>updated</from-outcome>
			<to-view-id>/painelPagina.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/formularioLinkInterno.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{linkInternoEntity.persist}</from-action>
			<from-outcome>persisted</from-outcome>
			<to-view-id>/painelPagina.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{linkInternoEntity.update}</from-action>
			<from-outcome>updated</from-outcome>
			<to-view-id>/painelPagina.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/formularioLinkExterno.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{linkExternoEntity.persist}</from-action>
			<from-outcome>persisted</from-outcome>
			<to-view-id>/painelPagina.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{linkExternoEntity.update}</from-action>
			<from-outcome>updated</from-outcome>
			<to-view-id>/painelPagina.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/formularioDownload.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{downloadEntity.persist}</from-action>
			<from-outcome>persisted</from-outcome>
			<to-view-id>/painelPagina.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{downloadEntity.update}</from-action>
			<from-outcome>updated</from-outcome>
			<to-view-id>/painelPagina.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/formularioImagem.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{imagemEntity.persist}</from-action>
			<from-outcome>persisted</from-outcome>
			<to-view-id>/painelPagina.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{imagemEntity.update}</from-action>
			<from-outcome>updated</from-outcome>
			<to-view-id>/painelPagina.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
