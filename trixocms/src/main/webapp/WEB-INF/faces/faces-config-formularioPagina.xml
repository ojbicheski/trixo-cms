<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
	<navigation-rule>
		<from-view-id>/formularioPagina.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{paginaEntity.persist}</from-action>
			<from-outcome>persisted</from-outcome>
			<to-view-id>/painelSite.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{paginaEntity.update}</from-action>
			<from-outcome>updated</from-outcome>
			<to-view-id>/painelSite.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
